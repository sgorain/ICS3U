package PACKAGENAME;
import java.util.Scanner;
public class CLASSNAME {
	public static void main(String [] args){
		Scanner sc = new Scanner(System.in); 	
		int input = sc.nextInt();
		isPrime(input);
	}
	
	public static boolean isPrime(int x){
		for (int i=2; i<x; i++){
			if(x%i==0){
				return false;
			}
		}
		return true;
		
	}

}
