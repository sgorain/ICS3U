package Gorain;

public class ArrayGame {

	public static void main(String[] args) {
		int [][] array = new int[20][20];
		for(int row = 0; row<array.length; row++){
			for(int column = 0; column < array[0].length; column++){
				System.out.println(array[row][column]);
			}
			System.out.println();
		}
	}

}
